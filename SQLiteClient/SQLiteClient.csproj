<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net48;net7.0;net8.0</TargetFrameworks>
		<Configurations>Debug;Release;ProdDebug</Configurations>
		<LangVersion>12</LangVersion>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Description>Layer provides SQLite Client implementation on top of System.Data.SQLite.Core </Description>
		<PackageId>TCore.SQLiteClient</PackageId>
		<Title>TCore SQLiteClient implementation</Title>
		<Version>1.2.0.0</Version>
		<Authors>Robert Little</Authors>
		<Company>Robert Little</Company>
		<Product>TCore.SQLiteClient</Product>
		<PackageProjectUrl>https://github.com/rlittletht/TCore.SQLiteClient</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="System.Data.SQLite.Core" Version="1.0.118.0" />
	  <PackageReference Include="TCore.SqlCore" Version="1.2.0" />
	</ItemGroup>
</Project>